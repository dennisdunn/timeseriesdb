{
    "name": "mongodb-timeseries",
    "version": "0.1.0",
    "description": "A timeseries database using Mongo.",
    "main": "src/tsutils.js",
    "scripts": {
        "start": "cd docker && docker-compose up -d --build",
        "stop": "cd docker && docker-compose down",
        "restart": "cd docker && docker-compose down && docker-compose up -d ",
        "build": "docker build -f ./docker/Dockerfile -t docker_node:latest .",
        "test": "mocha ./spec/**/*.js"
    },
    "keywords": [
        "timeseries",
        "mongo"
    ],
    "author": "Dennis Dunn <ansofive@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "moment": "^2.22.1",
        "util": "^0.10.3"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "mocha": "^5.1.1"
    }
}